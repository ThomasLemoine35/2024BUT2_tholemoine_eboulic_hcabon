<script>
    
    let produits=<
    


const detProduits = document.getElementById('div_equipements');

for (let equipments of produits) {
    const equipArt = document.createElement('article');
    detProduits.append(equipArt);

    const titleH3 = document.createElement('h3');
    titleH3.innerText = equipments.type;
    equipArt.append(titleH3);
    
    const para = document.createElement('p');
    para.innerText = equipments.description;
    para.classList.add('desc-equipment');
    equipArt.append(para);

   
}

</script>

